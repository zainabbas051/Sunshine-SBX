  <aura:attribute name="ClientsCaseManager" type="string" default="" />
  <aura:attribute name="AdditionalNotes" type="string" default="" />
  <aura:attribute name="ClientSatisfactionObj" type="Client_Satisfaction_Survey__c" default="{ 
